<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="eus.ehu.gleonis.gleonismastodonfx.db.DBAccount" table="DBAccount">
        <id name="id" type="java.lang.String"/>

        <property name="username" type="java.lang.String"/>
        <property name="avatarUrl" type="java.lang.String"/>
        <property name="last_access" type="java.lang.String"/>
        <property name="token" type="java.lang.String"/>

    </class>
</hibernate-mapping>